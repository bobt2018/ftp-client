{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "noEmit": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "target": "es5",
    "lib": [
      "es2016",
      "dom"
    ],
    "paths": {
      "@angular/*": [
        "node_modules/@angular/*"
      ],
      "@dream/*": [
        "node_modules/@dream/*"
      ],
      "@ngrx/*": [
        "node_modules/@ngrx/*"
      ],
      "rxjs/*": [
        "node_modules/rxjs/*"
      ]
    },
    "typeRoots": [
      "node_modules/@types"
    ]
  }
}